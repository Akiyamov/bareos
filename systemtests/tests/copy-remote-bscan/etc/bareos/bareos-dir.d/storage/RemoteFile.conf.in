Storage {
  Name = RemoteFile
  Address = @hostname@                # N.B. Use a fully qualified name here (do not use "localhost" here).
  Password = "@sd_password@"
  Device = RemoteFileStorage
  Media Type = File
  SD Port = @sd2_port@
}

Storage {
  Name = RemoteFile2
  Address = @hostname@                # N.B. Use a fully qualified name here (do not use "localhost" here).
  Password = "@sd_password@"
  Device = RemoteFileStorage2
  Media Type = File
  SD Port = @sd2_port@
}
